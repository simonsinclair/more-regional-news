<h2 class="mrn__title">More regional news</h2>
<ul>
{{#carouselItems}}
	<li class="item">
		<img src="{{img}}" alt="" width="" height="" />
		<div class="item__caption">
			<p>{{title}}</p>
			<ul class="item__footer">
				<li class="item__timestamp">{{timestamp}}</li>
				<li class="item__region">{{region}}</li>
			</ul>
		</div>
	</li>
{{/carouselItems}}
</ul>
